config global
    option chipset          'sx1302'
    option enabled          '0'

config sx1302
    option model            'rak_5146'
    option region           'EU868'
    option channel_plan     'eu868'
    option gnss             '1'
    option usb              '1'
    option antenna_gain     '2'
    option event_bind       'ipc:///tmp/concentratord_event'
    option command_bind     'ipc:///tmp/concentratord_command'
    option com_dev_path      '/dev/ttyACM0'
    option gnss_dev_path    'gpsd://localhost:2947'

